{
    "name": "brifiction/owlet",
    "description": "An example simple Laravel package",
    "keywords": [
        "laravel",
        "composer",
        "package"
    ],
    "homepage": "https://entwurfhaus.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Ng",
            "email": "brianngchongeu@gmail.com",
            "homepage": "https://entwurfhaus.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "illuminate/database": "^7.0",
        "illuminate/events": "^7.0",
        "phpunit/phpunit": "^9.2"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Entwurfhaus\\Owlet\\ExampleServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Entwurfhaus\\Owlet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/Unit/",
            "Tests\\Feature\\": "tests/Feature/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "phpunit tests"
        ]
    }
}
